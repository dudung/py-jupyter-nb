    "def zero(rows, cols, val=0):\n",
    "    m = [[val for _ in range(cols)] for _ in range(rows)]\n",
    "    return m\n",
    "\n",
    "def print_mat2(m):\n",
    "    for r in m:\n",
    "        print(r)"
