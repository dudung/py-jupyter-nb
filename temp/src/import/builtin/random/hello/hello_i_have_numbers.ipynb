   "source": [
    "import random\n",
    "\n",
    "rep = 4\n",
    "\n",
    "for i in range(0, rep):\n",
    "    \n",
    "    N = random.randint(2, 9)\n",
    "    nums = []\n",
    "    for i in range(N):\n",
    "        x = random.randint(0, 9)\n",
    "        nums.append(x)\n",
    "    print(\"Hello, I have\", N, \"numbers and they are\", nums)"
   ]
