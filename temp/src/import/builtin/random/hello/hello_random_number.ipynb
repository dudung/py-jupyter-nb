    "import random\n",
    "\n",
    "N = 10\n",
    "for i in range(N):\n",
    "    # generate random number from 1000 to 9999\n",
    "    num = random.randint(1000, 9999)\n",
    "    \n",
    "    # greet the number\n",
    "    print(\"Hello, \", num, \"! How are you?\", sep='')"
